{
  "expo": {
    "name": "PartyPlay",
    "slug": "PartyPlay",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.company",
      "config": {
          "googleMapsApiKey": "<ios maps api key>"
      }
    },
    "android": {
      "package": "com.company",
      "config": {
          "googleMaps": {
              "apiKey": "AIzaSyAHnm2KoWjehZ9aWBPvntAAIZeYzQIxEvM"
          }
      }
    },
    "androidStatusBar":{
      "hidden": true
    }
  }
}
